{
  "name": "contact-management",
  "version": "1.0.0",
  "main": "dist/app.js",
  "private": true,
  "scripts": {
    "start": "node dist/app.js",
    "build": "npm run clean && tsc",
    "start:dev": "ts-node-dev --files --respawn ./src/app.ts",
    "tc": "tsc --noEmit",
    "lint": "eslint 'src/**'",
    "lint:fix": "eslint --fix 'src/**'",
    "pretty": "prettier --write '**'",
    "pre-commit": "lint-staged",
    "commit-lint": "commitlint",
    "clean": "rm -rf ./dist",
    "prepare": "husky install"
  },
  "author": "Taranbir Singh",
  "dependencies": {
    "@adhityan/gc-doc": "^4.1.0",
    "@adhityan/gc-logger": "^3.1.7",
    "@sendgrid/mail": "^7.7.0",
    "axios": "^0.24.0",
    "body-parser": "^1.19.1",
    "class-validator": "^0.13.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "http-graceful-shutdown": "^3.1.5",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.41",
    "typeorm-typedi-extensions": "^0.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.27",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-sort-class-members": "^1.14.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "prettier": "^2.5.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
